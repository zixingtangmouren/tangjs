{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "declaration": true,
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
