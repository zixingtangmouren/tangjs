{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "declaration": true,
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
